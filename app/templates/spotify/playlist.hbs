{{#with model as |playlist|}}
    {{playlist.name}}
{{/with}}
{{#each model as |track| }}
    {{#paper-card as |card|}}
        {{#card.title as |title|}}
            {{#title.text as |text|}}
                {{#text.headline}}{{track.name}}{{/text.headline}}
                {{#text.subhead}}{{track.artists}} - {{track.album}}{{/text.subhead}}
            {{/title.text}}
            {{title.media size="sm" src=track.image alt=track.name title=track.name}}
        {{/card.title}}
        {{#card.actions}}
            {{#paper-button}}Action 1{{/paper-button}}
            {{#paper-button}}Action 2{{/paper-button}}
        {{/card.actions}}
    {{/paper-card}}
{{/each}}

{{outlet}}


        
      